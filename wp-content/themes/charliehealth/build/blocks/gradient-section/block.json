{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "acf/gradient-section-block",
  "title": "Animated Gradient Section",
  "description": "Background color change",
  "style": "file:./index.css",
  "script": "file:./index.js",
  "category": "custom",
  "icon": "editor-quote",
  "apiVersion": 2,
  "acf": {
    "mode": "preview",
    "renderTemplate": "gradient-section.php"
  },
  "supports": {
    "color": {
      "background": true,
      "text": false,
      "enableContrastChecker": false,
      "gradients": false,
      "link": false
    }
  }
}