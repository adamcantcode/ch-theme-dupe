{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "layout": {
      "contentSize": "1280px"
    },
    "color": {
      "custom": false,
      "defaultPalette": false,
      "text": false,
      "customGradient": false,
      "background": true,
      "palette": [
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        },
        {
          "slug": "lightest-purple",
          "color": "#F8F8FF",
          "name": "Lightest Purple"
        },
        {
          "slug": "light-purple",
          "color": "#F0EFFC",
          "name": "Light Purple"
        },
        {
          "slug": "cream",
          "color": "#F8F4F0",
          "name": "Cream"
        },
        {
          "slug": "lightest-teal",
          "color": "#ecf7fa",
          "name": "Lightest Teal"
        }
      ]
    },
    "spacing": {
      "blockGap": false,
      "customSpacingSize": false,
      "margin": false,
      "padding": true,
      "units": ["rem"],
      "spacingSizes": [
        {
          "slug": "xs",
          "name": "XS",
          "size": "xs"
        },
        {
          "slug": "sm",
          "name": "SM",
          "size": "sm"
        },
        {
          "slug": "md",
          "name": "Regular",
          "size": "md"
        },
        {
          "slug": "lg",
          "name": "LG",
          "size": "lg"
        },
        {
          "slug": "xl",
          "name": "XL",
          "size": "xl"
        }
      ]
    }
  }
}
