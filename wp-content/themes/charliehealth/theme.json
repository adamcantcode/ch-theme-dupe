{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "custom": false,
      "defaultPalette": false,
      "text": false,
      "customGradient": false,
      "background": true,
      "palette": [
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        },
        {
          "slug": "light-purple",
          "color": "#f0effc",
          "name": "Light Purple"
        },
        {
          "slug": "cream",
          "color": "#F8F4F0",
          "name": "Cream"
        }
      ]
    },
    "layout": {
      "contentSize": "1280px"
    },
    "blocks": {
      "core/group": {
        "typography": {
          "fontSizes": [],
          "customFontSize": false,
          "dropCap": false,
          "fluid": false,
          "fontFamilies": [],
          "fontStyle": false,
          "fontWeight": false,
          "letterSpacing": false,
          "lineHeight": false,
          "textColumns": false,
          "textDecoration": false,
          "textTransform": false
        },
        "spacing": {
          "blockGap": false,
          "customSpacingSize": false,
          "margin": false,
          "padding": true,
          "units": [
            "rem"
          ],
          "spacingSizes": [
            {
              "slug": "sm",
              "name": "SM",
              "size": "var(--wp--custom--spacing--sm)"
            },
            {
              "slug": "md",
              "name": "MD",
              "size": "var(--wp--custom--spacing--md)"
            },
            {
              "slug": "lg",
              "name": "LG",
              "size": "var(--wp--custom--spacing--lg)"
            }
          ]
        },
        "color": {
          "gradients": [],
          "defaultGradients": false,
          "customGradient": false
        },
        "typography": {
          "customFontSize": false,
          "dropCap": false,
          "fluid": false,
          "fontFamilies": [],
          "fontSizes": [],
          "fontStyle": false,
          "fontWeight": false,
          "letterSpacing": false,
          "lineHeight": false,
          "textColumns": false,
          "textDecoration": false,
          "textTransform": false
        },
        "layout": {
          "contentSize": "768px",
          "wideSize": "1000px"
        }
      }
    }
  }
}
