{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "acf/section-container-block",
  "title": "Section & Container",
  "description": "Used for creating sections, adjusting padding, BG color and defining the content width",
  "category": "custom",
  "icon": "editor-insertmore",
  "style": "file:./index.css",
  "script": "file:./index.js",
  "acf": {
    "mode": "preview",
    "renderTemplate": "section-container.php"
  },
  "supports": {
    "align": ["full"],
    "anchor": true,
    "color": {
      "background": true,
      "text": false,
      "enableContrastChecker": false,
      "gradients": true,
      "link": false
    },
    "spacing": {
      "padding": [
        "top",
        "bottom"
      ]
    }
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    }
  }
}
